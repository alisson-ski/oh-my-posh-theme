{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "foreground": "#45F1C2",
          "template": "\uf366 alisson on"
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#0CA0D8",
          "template": " \ue5ff {{ .Path }} ",
          "properties": {
            "folder_separator_icon": "/",
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "powerline_symbol": "\ue0b0",
          "foreground": "#14A5AE",
          "template": "\uf09b {{ .HEAD }}",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "angular",
          "style": "plain",
          "foreground": "red",
          "template": " \ue753 {{ .Full }} "
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "#6CA35E",
          "template": "\ue718 {{ .Full }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#cd5e42",
          "template": "\ue3bf "
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#CD4277",
          "template": "# "
        }
      ],
      "newline": true
    }
  ]
}
